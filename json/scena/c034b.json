[
  {
    "Text": [
      "マリエッタさんも\u0001",
      "忙しいみたいだし……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "家族全員で食事をするのは\u0001",
      "しばらく先のことになりそうね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そういえばユンクス、\u0001",
      "マリエッタさんは帰国の目処は\u0001",
      "ついているのかしら？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "うーん、しばらく難しいかもな。\u0001",
      "結構忙しくしてるみたいだし……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そう……\u0001",
      "家族全員で食事をするのは\u0001",
      "しばらく先のことになりそうね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "仕事で忙しい親父が、\u0001",
      "久しぶりに夕飯の時間に\u0001",
      "帰ってきたんだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ありゃ、そういえば\u0001",
      "イスが足りないな。\u0001",
      "物置から出してこないと……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ありゃ、そういえば\u0001",
      "イスが足りないな。\u0001",
      "物置から出してこないと……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "アンリ、オルキスタワーを\u0001",
      "近くで見てきたんでしょ。\u0001",
      "どんな感じだった？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "やっぱり大っきかった？\u0001",
      "ミラがかかってそうだった？\u0001",
      "いっぱい人いた？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ちょ、ちょっと姉さん。\u0001",
      "一度にそんなに質問されても\u0001",
      "答えられないってば～。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "いいじゃない、\u0001",
      "ケチケチしないで教えてよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "もぐもぐ……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "あ、支援課の皆さん。\u0001",
      "ウチになにか用ですか？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "よかったら一緒に\u0001",
      "夕飯を食べていきます？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "それにしても昼の除幕式は\u0001",
      "すごかったですよね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "警察のみなさんは\u0001",
      "忙しいでしょうけど、\u0001",
      "がんばってくださいね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "妻のマリエッタは\u0001",
      "外国のＩＢＣ支社で働いてるんだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あっちはあっちで忙しそうだし、\u0001",
      "帰ってくるのはまだまだ先だろうな……\u0002"
    ],
    "Translation": ""
  }
