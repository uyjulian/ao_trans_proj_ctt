  {
    "Text": [
      "\u0007\u0005",
      "『お菓子の王国　第一巻』がある。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "",
    "Translation": ""
  },
  {
    "Text": [
      "\u0007\u0002",
      "『ミックスジェラート』",
      "\u0007\u0000",
      "のレシピを覚えた。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "話をする\u0001",
      "買い物をする\u0001",
      "やめる\u0001"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "こんな時間に\u0001",
      "ウロウロしてるなんて\u0001",
      "珍しいじゃないか。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "一応、閉店時間は\u0001",
      "過ぎちまってんだが……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ま、おまえらならいいだろ。\u0001",
      "遠慮せず見てってくれ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00000F",
      "サンキュー、オスカー。\u0001",
      "是非そうさせてもら……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00603F",
      "…………………………\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00006F",
      "（か、買い物するにしても\u0001",
      "  早く済ませた方がよさそうだな。）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "一応、閉店時間は\u0001",
      "過ぎちまってんだが……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ま、おまえらならいいだろ。\u0001",
      "遠慮せず見てってくれ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "さーて、そろそろ\u0001",
      "後片付けに入るとするか。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "パン屋の仕込みは、\u0001",
      "早朝３時からなんでな。\u0001",
      "明日に備えてさっさと休まねえと。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "私の作ったパン、\u0001",
      "オスカーのに\u0001",
      "売り上げで負けてる……！？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ぐぬぬ、明日こそは……！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "話をする\u0001",
      "買い物をする\u0001",
      "やめる\u0001"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "おやいらっしゃい、\u0001",
      "何か入用かい？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "店はもう少し\u0001",
      "開けてるつもりだから、\u0001",
      "ゆっくりしてていいからね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふふ、モモったらよっぽど\u0001",
      "楽しかったんでしょうね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……それでね、それでね。\u0001",
      "リュウ君、オルキスタワーに\u0001",
      "かってに入ろうとしたの。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そしたら、モモとアンリちゃんまで\u0001",
      "警察の人におこられちゃって……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あらあら……\u0001",
      "それは大変だったわねえ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "でもね、でもね。\u0001",
      "とっても楽しかったの……！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "でもリュウ君、面白かったの。\u0001",
      "ぜんぶアンリちゃんのせいに\u0001",
      "しようとしちゃって……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "でも、すぐにばれちゃって、\u0001",
      "もっと叱られちゃったの。\u0001",
      "くすくす……\u0002"
    ],
    "Translation": ""
  }
]