[
  {
    "Text": "イェ、イェ～～～ッ！！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "不安なんか知らねェ～～！！\u0001",
      "ワケわかんねえ事に\u0001",
      "かまってるヒマねェ～！！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヤァ、歌でも歌って吹き飛ばせ、\u0001",
      "イィ～～～～～ヤッホォウ～～！！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あんなデカい木なんざ屁でもねェ～！\u0001",
      "不安なんか知らねェ～～！！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヤァ、歌でも歌って吹き飛ばせ、\u0001",
      "イィ～～～～～ヤッホォウ～～！！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヴオ゛オ゛ォォォ……\u0001",
      "がぁくそッ、何かイライラすんぜェ～ッ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "マインツで襲撃事件だぁ！？\u0001",
      "んなことより、\u0001",
      "ヴァルドさんはどこだァア～ッ！！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ハンッ、言っておくが、\u0001",
      "ゲンコは貸しだからなァッ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヴァルドさん……\u0001",
      "カジノで大当たりでもしたのか？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "でも、だったら何で\u0001",
      "教えてくれねえんだよォオ～！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ハァ～、にしてもヴァルドさんが\u0001",
      "いねえとマジつまンねえぜ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヴァルドさ～～んッ！\u0001",
      "マジお願いだから顔出してくれよォ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "酒ならオレらが買うから\u0001",
      "ここで飲みましょうよォオ～～ッ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "アンタがいねえと、\u0001",
      "何も始まらねえンだよォオオ～～ッ！！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "酒ならオレらが買うから\u0001",
      "ここで飲みましょうよォオ～～ッ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "アンタがいねえと、\u0001",
      "何も始まらねえンだよォオオ～～ッ！！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "信じねえ……\u0001",
      "オレはぜってェ信じねえぞッ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そんなフザケた話が\u0001",
      "あってたまるかァア……！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "イェ～、イャアア～～ッ！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "テスタメンツのヤロウどもォオ！\u0001",
      "スカしてられるのも今の内ィイッ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#4S",
      "……イィィ～～ヤッホウッッ！！",
      "#3S",
      "\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "テスタメンツのヤロウどもォオ！\u0001",
      "スカしてられるのも今の内ィイッ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#4S",
      "……イィィ～～ヤッホウッッ！！",
      "#3S",
      "\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "で、オマエら。\u0001",
      "調査の結果はどうだったんだ？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "オ、オッス……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "１週間前になるんスけど、\u0001",
      "カノンってガキがヴァルドさんを\u0001",
      "見かけたらしくって……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "それで、その時にどうやら\u0001",
      "エニグマっつう小型の通信端末を\u0001",
      "使っていたみたいッス。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "交換屋の女主人が言うには\u0001",
      "１０万ミラはする高価な\u0001",
      "シロモンらしいんスけど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "じゅ、１０万ミラァ～！？\u0002",
    "Translation": ""
  },
  {
    "Text": "うおぅ、マジか……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ヴァ、ヴァルドさん、\u0001",
      "一体どこにそんなミラが……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "確かにそれも気になるが……\u0001",
      "コウキ、オマエ通信端末っつったな？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "それはつまり……\u0001",
      "ヴァルドさんが誰かと\u0001",
      "連絡を取ってたってことかよ？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ハイ、流石にそのガキんちょも\u0001",
      "内容までは聞き取れなかった\u0001",
      "そうなんスけど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ええ、でもヴァルドさん、\u0001",
      "何だか楽しそうに笑ってたって……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "……わ、笑ってた？\u0002",
    "Translation": ""
  },
  {
    "Text": "何か良いことあったんすかね……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ってか、まさかオレたち\u0001",
      "ヴァルドさんに見捨てられた……\u0001",
      "なぁんてことはないッスよね？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "…………………………\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "てめえ、スラッシュ。\u0001",
      "ふざけたクチ利いてんじゃねえぞ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ブチ殺されたいなら別だけどよォ？\u0002",
    "Translation": ""
  },
  {
    "Text": "す、すんません……！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "#00001F",
      "（ヴァルドがエニグマを、か……\u0001",
      "  確かに少し気になるな。）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00203F",
      "（ええ、言ってもまだエニグマは\u0001",
      "  一般的にそれほど数が\u0001",
      "  出回ってるワケでもないですし……）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10303F",
      "（……ヴァルド………………）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "おう、ジェド。\u0001",
      "コウキとディーノのヤロウは\u0001",
      "まだ来ねえのかァ～？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "まあ、待ってやれよ。\u0001",
      "追加調査が必要なんだとさ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ところで、お前らの方は\u0001",
      "全然だったそうじぇねえか。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "まあよォ……\u0002",
    "Translation": ""
  },
  {
    "Text": "す、すんません。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "一通り心当たりは\u0001",
      "回ったんスけど、ここ最近は\u0001",
      "モノの見事に誰も見てないって……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "……そうか。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ヴァルドさん……\u0001",
      "マジでどこへ行ったんだろうな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あ～っと、オレたちは\u0001",
      "とにかくヴァルドさんの\u0001",
      "行きそうな場所を探すかンな～。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "範囲は街ゼンブだ。\u0001",
      "片っ端から調べて回りやがれ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "オッス！\u0002",
    "Translation": ""
  },
  {
    "Text": "かしこまりッス！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "あ～、ヴァルドさんを見かけたら\u0001",
      "ソッコーでここに戻って報告しろよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "テメエらが声掛けても\u0001",
      "またシカトされンのがオチだかンなァ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "オッス！\u0002",
    "Translation": ""
  },
  {
    "Text": "かしこまりッス！\u0002",
    "Translation": ""
  },
  {
    "Text": "んで、ヤツら何つったんだァ？\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ハ、ハイ。\u0001",
      "それが素直に謝って来たんスよ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そういう態度に出られると\u0001",
      "それ以上ケチ付けられねえっつうか……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ったく、どーしようもねェなァッ！\u0001",
      "青坊主のヤツらもヤツらだが、\u0001",
      "てめえらも気合い足りねえンだよォ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "いーか、そういう時は挑発だァア。\u0001",
      "今からオレがとっておきの脅し文句を\u0001",
      "教えてやるから、よォ～く覚えとけ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "『オマエのかーちゃん、デーベーソー！』\u0001",
      "……そらッ、言ってみろ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "え、え～っと……\u0002",
    "Translation": ""
  },
  {
    "Text": "オ、オスッ！\u0002",
    "Translation": ""
  },
  {
    "Text": "オ、オマエのかーちゃん……\u0002",
    "Translation": ""
  },
  {
    "Text": "デ、デーベーソー……っと。\u0002",
    "Translation": ""
  },
  {
    "Text": "オラオラァ、声が小せえぞォォオ～！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "コウキ……\u0001",
      "その話は本当なんだろうな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ええ、ガキんちょどもの\u0001",
      "言ってることッスけど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "オレァ信じらんねえ、\u0001",
      "ってか信じてたまるかァッ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あのヴァルドさんが\u0001",
      "一方的にやられるなんざァ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00105F",
      "（これって……\u0001",
      "  雨の日の対決のことを\u0001",
      "  言っているのかしら？）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00001F",
      "（ああ……だろうな。）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10303F",
      "（………………）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "て、てめえ……\u0001",
      "どのツラ下げてここに来た！？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ワ、ワジ……\u0002",
    "Translation": ""
  },
  {
    "Text": "ヒャハ、今度は俺たちってかァ？\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "いいぜ、オモテ出ろやァ！\u0001",
      "返り討ちにしてやらァアア～ッ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "お、おい……！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "――止めろ、ルガノフ！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ガキどもの話が本当だとしても、\u0001",
      "ここで手を出したらヴァルドさんの\u0001",
      "顔を潰すことになる……！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "それに、そんなことをしても\u0001",
      "ヴァルドさんは喜ばねえぞ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ちいッ、クソがァ……！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "……どういうつもりで\u0001",
      "ここへ来たかは知らねえが。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "争う気がないってんなら、\u0001",
      "さっさと出てってくれねえか？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "もっとも、喧嘩売りに\u0001",
      "来たってんなら話は別だけどよ？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10303F",
      "いや、そんなつもりはないさ。\u0002",
      "\u0003",
      "#10300F",
      "邪魔したね。\u0001",
      "……行こうか、みんな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "あ、ああ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "俺はまだ、ヴァルドさんを\u0001",
      "許したわけじゃねえ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……わけじゃねえが……\u0001",
      "サーベルバイパーってとこが\u0001",
      "すげえ楽しい場所だったのは確かだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "だったらバイパーの幹部として……\u0001",
      "守っていくのがスジってもんだよな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "俺はまだ、ヴァルドさんを\u0001",
      "許したわけじゃねえが……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "バイパーの幹部として、\u0001",
      "この大切な場所を守っていくのが\u0001",
      "スジってもんだよな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……くそっ。\u0001",
      "よりによって青坊主と\u0001",
      "協力するなんざ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ルガノフ、コウキ、ディーノ……\u0001",
      "アイツらなに考えてやがる……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ん？　支援課か……\u0001",
      "どうやらお互い進展はなさそうだな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "いいか、今度\u0001",
      "ヴァルドさんを見かけたら\u0001",
      "すぐにオレたちに声を掛けろよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……俺たちが言いたいことは、\u0001",
      "ルガノフが大体言ってくれた。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "他に話すことはねえ。\u0001",
      "……今日の所は失せるんだな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10303F",
      "…………………………\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "俺たちが話すことはもうねえ。\u0001",
      "……今日の所は失せるんだな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヴァルドさん……\u0001",
      "一体アンタに何があったんだ？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヴァルドさん……\u0001",
      "マジでどこへ行ったんだろうな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "争う気がないなら、\u0001",
      "さっさと出て行きやがれ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "これ以上いられると、\u0001",
      "正直オレもどうなるか分からねえ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "いいか、ワジ……\u0001",
      "このまま無事でいられると\u0001",
      "思うなよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あいつら……\u0001",
      "よりによって青坊主どもに\u0001",
      "協力なんてしやがって。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……ジェドさん。\u0001",
      "やっぱり俺たちも協力したほうが\u0001",
      "よくないッスか？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヒャハ、何言ってんだヒューイ！\u0001",
      "バイパーとしちゃ、裏切りモンは\u0001",
      "きちんとシメてやって……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……バイパーは解散したって\u0001",
      "言ってるだろうが、バカ野郎！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ス、スンマセン。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "……とにかく、今は考え中だ。\u0001",
      "お前らも勝手なことはするなよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ちっ、よく分かんねえが\u0001",
      "騒がしいことになってんな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "はい、街の方もマインツ占領の\u0001",
      "話題一色って感じッスからね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "まさか、ヴァルドさん……\u0001",
      "事件に巻き込まれてたり\u0001",
      "していないッスよね？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "まあ流石にそれはないと思うが……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "とにかくテメエら、\u0001",
      "今日も気合い入れて情報を集めんぞ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#2P",
      "オッス！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#3P",
      "オッス！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヴァルドさん……\u0001",
      "今頃どうしてるんスかね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "さあな……\u0001",
      "しばらく寝ぐらの方にも\u0001",
      "帰っていないようだが……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ちらっとも見かけなくなって、\u0001",
      "もう３日になるんスよね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "港湾区の方でヒューイさんたちが\u0001",
      "見かけたのを最後に……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ああ、それも声を掛けたのに\u0001",
      "無視されたっつう話だったな……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……ともかく、ヴァルドさんに\u0001",
      "何かあったってんならコトだ。\u0001",
      "手分けして心当たりを探すぞ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "お前ら２人はこの旧市街だ。\u0001",
      "色んなヤツに片っ端から聞いて回れ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "オッス！\u0002",
    "Translation": ""
  },
  {
    "Text": "了解ッス！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "（ヴァルド……\u0001",
      "  所在が分からないのか……？）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10303F",
      "（何だろう……気になるね。）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "イグニスには、俺とルガノフが\u0001",
      "交代で詰めておくからな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "何かあったらすぐに報告に来いよ。\u0002",
    "Translation": ""
  },
  {
    "Text": "オッス！\u0002",
    "Translation": ""
  },
  {
    "Text": "了解ッス！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "どうだ、昨日はあれから\u0001",
      "ヴァルドさんの姿を見たか？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "いえ、俺の方は全然……\u0002",
    "Translation": ""
  },
  {
    "Text": "お、俺もッス。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "そうか、昨日の様子からすりゃ\u0001",
      "二日酔いって所だとは思うが……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そうだな……後でまた\u0001",
      "寝ぐらの様子でも見に行くか。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "お前らも付いて来いよ。\u0002",
    "Translation": ""
  },
  {
    "Text": "オッス！\u0002",
    "Translation": ""
  },
  {
    "Text": "もちろんッス！\u0002",
    "Translation": ""
  },
  {
    "Text": "そうか……ヴァルドさんが……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ええ、人目もはばからず、\u0001",
      "フラフラしながら店の方に……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "相当な量を買い込んでいたから\u0001",
      "今頃また寝ぐらの方で\u0001",
      "呑んでいるんじゃないかと……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "正直……\u0001",
      "あんなヴァルドさんの姿、\u0001",
      "見たくなかったッス。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "まさか、あの人が……\u0001",
      "酒に呑まれてしまうなんて……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "……ああ、俺も同じ気持ちだ。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ともかく……\u0001",
      "路上で酔い潰れたりされると\u0001",
      "色々マズイことになりそうだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "引き続き、\u0001",
      "全員で気に掛けていくぞ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "オッス！\u0002",
    "Translation": ""
  },
  {
    "Text": "了解です！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "ヴァルドがそんなことに……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#12P",
      "#00301F",
      "いわゆる自棄",
      "#4Rヤ　ケ#",
      "酒っつうヤツか。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00103F",
      "あまり大量だと\u0001",
      "身体の方が心配だけど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#6P",
      "#10308F",
      "………………………………\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "いいか、何かあったら\u0001",
      "すぐに俺かルガノフを呼べよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "オッス！\u0002",
    "Translation": ""
  },
  {
    "Text": "了解です！\u0002",
    "Translation": ""
  },
  {
    "Text": "お、お前ら……！\u0002",
    "Translation": ""
  },
  {
    "Text": "特務支援課……それにワジ！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "……どうやら、ヴァルドさんには\u0001",
      "まだ会っていないようだな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10305F",
      "ヴァルドがどうかしたのかい？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "そ、それは……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "黙ってろ、ディーノ。\u0001",
      "サツに尻尾を振る腰抜け野郎に\u0001",
      "話すことなんて何もねえ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "分かったらとっとと失せやがれ。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "#10303F",
      "……やれやれ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "（なんだかんだ言って、\u0001",
      "  ジェドさんも迷ってるみたいだな……）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "正体不明の武装集団か……\u0001",
      "もし襲われたとしても流石に\u0001",
      "返り討ちってワケにはいかねえよな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ルガノフさん……\u0001",
      "マジでかっこよかったな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "１０万ミラって、\u0001",
      "そんな大金どうやったら……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "色々聞いて回ったんスけど……\u0001",
      "ここ最近は、マジで誰も\u0001",
      "見てないって言うんスよね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "流石に街を出たとは\u0001",
      "思いたくないッスけど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ハッ……\u0001",
      "さっきはおかしなヤツに\u0001",
      "気を削がれちまったが……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……次はねえからな。\u0001",
      "覚えてろよ、ワジ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10303F",
      "………………………………\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……次はねえからな。\u0001",
      "覚えてろよ、ワジ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "て、てめえ……\u0001",
      "もっとマシなことを考えろや！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "で、いい案は思いついたかよ？\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "おうよ！　実は今朝ここへ来る前に\u0001",
      "パンを山ほど抱えたシスターを\u0001",
      "見かけたんだけどよォ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "それがもう、可愛いの何のってよォ！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "……アア？　その話がどう\u0001",
      "ヴァルドさんの機嫌取りに\u0001",
      "つながるってんだ？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ああ、だから\u0001",
      "今度そのシスターをナンパしに\u0001",
      "教会に行ってだなァ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "てめえ、頭おかしいだろ！？\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "何で俺たちが教会になんか\u0001",
      "行かなきゃならねえンだよ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ハァ、今朝のヴァルドさん、\u0001",
      "マジでおっかなかったよなァ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "っつうかさ……\u0001",
      "ヴァルドさんは何であそこまで\u0001",
      "ワジの野郎にこだわるんだ？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "とにかく一番厄介なヤツが\u0001",
      "いなくなったんだ。\u0001",
      "むしろ喜ぶべきっつうか……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "おいコラ、スラッシュ！\u0001",
      "滅多なこと口にしてンじゃねえぞ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "オレたちがヴァルドさんに\u0001",
      "意見しようなんざ百万年早ェんだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "んなことより、どうすりゃ\u0001",
      "キゲン直してもらえるか考えろや！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あ、ああ。\u0001",
      "んなこたァ分かってるがよォ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ったく、今みたいな話を\u0001",
      "ヴァルドさんに聞かれてみろ。\u0001",
      "半殺しじゃすまねえぞ！？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "わ、悪かったよ。\u0001",
      "もう言わねえって……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ヒャハ、怒られちまった……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "えっと……\u0001",
      "トンカチと釘はどこにやったっけ？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "念のため、\u0001",
      "棍棒を増強しとかねえとなァ～。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "くう～、やっぱ\u0001",
      "先輩方はサイコーだよな！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "だァもー、ワケ分かんなって来た。\u0001",
      "誰か説明してくれよォ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "相変わらず、寝ぐらの方にも\u0001",
      "帰っていないみたいッスけど……\u0001",
      "どこで寝泊りしてんスかね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヒャハ、世の中には\u0001",
      "ヘンな野郎がいるもんだなァ～。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……あれ、そういえば俺とヒューイ、\u0001",
      "ケンカしてなかったっけか。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "……まあ、小せえことはいいかァ。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "今はヴァルドさんのことを\u0001",
      "考えねえとなァ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そ、そういうてめえこそ\u0001",
      "ロクな案ださねえくせによォ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヴァルドさんが戻ってくるまでは、\u0001",
      "幹部のジェドさんとルガノフさんが\u0001",
      "サーベルバイパーを引っ張ることになったんだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "皆で協力して、バイパーを\u0001",
      "守っていかなくちゃな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヴァルドさん、\u0001",
      "今頃何してんだろうな……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "よく考えりゃ、幹部のお２人は\u0001",
      "サーベルバイパー結成当時からの\u0001",
      "メンバーなんだよな……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "俺たちなんかよりも\u0001",
      "ずっとヴァルドさんのことを……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヴァルドさん……\u0001",
      "オレら以外にいったい誰と……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "は、早くどっか行けよ！\u0002",
    "Translation": ""
  },
  {
    "Text": "ヴァルドさん……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ヴァルドさん……\u0001",
      "やっぱり今日も見かけないッスね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そうだな……\u0001",
      "何かスゲエ騒ぎにもなってんし。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……とにかく、\u0001",
      "無事でいてくれりゃいいんだが。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ヴァルドさん……みんな……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "#00108F",
      "……ディーノ君…………\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "……なんだ、特務支援課か。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "#00003F",
      "他のメンバーは……\u0001",
      "まだ入院中なんだってな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ああ、オレ以外のメンバーはみんな……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "特にコウキさんが酷くて……\u0001",
      "……今もみんなとは別の病室で……\u0001",
      "うぅ、ひぐっ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00008F",
      "すまない……少し無神経だったな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ひぐっ……べ、別に……\u0001",
      "俺なんかを気遣う必要なんてねえよ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "先輩たちはツライ思いを\u0001",
      "してるってのに俺だけ無事で……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "うぅ……くそっ！\u0001",
      "なんで……なんでこんなことに……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00208F",
      "（ロイドさん……\u0001",
      "  今は１人にしてあげましょう。）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00003F",
      "（ああ……そうだな。）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10308F",
      "（…………………………）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ヴァルドさん……みんな……\u0002",
    "Translation": ""
  },
  {
    "Text": "なんで……なんでこんなことに……\u0002",
    "Translation": ""
  },
  {
    "Text": "グノーシス、か……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ヴァルドさん、\u0001",
      "ルバーチェもいないってのに\u0001",
      "あんなのを一体どこで……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "へへっ、やっぱり先輩方は偉大だな。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "こんな時になんだけど……\u0001",
      "俺、何だかちょっとだけ嬉しいや。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "うぅ……ヴァルドさん。\u0001",
      "まさか危険な仕事でもしてるんじゃ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ジェ、ジェドさんの言う通り\u0001",
      "話すことなんて何もないからなっ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#11P",
      "サツが……また来たのかよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#11P",
      "もしかして、新しい情報でも……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#11P",
      "……って、お前はワジ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#6P",
      "#10308F",
      "…………………………\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#5P",
      "て、てめえ……！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#5P",
      "よくも、ぬけぬけと……\u0001",
      "てめえのせいでヴァルドさんは！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#12P",
      "#00001F",
      "（くっ……さすがに険悪なムードだな。）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#6P",
      "#00208F",
      "（来たのはまずかったでしょうか……）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#5P",
      "…………………\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#11P",
      "おい、ルガノフ……？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#6P",
      "#10310F",
      "…………っ………………\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#12P",
      "#00011F",
      "お、おい……！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#6P",
      "#10111F",
      "ワジ君……！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#4S",
      "#11P",
      "よぉ、ワジィ！\u0001",
      "分かってンよなァ～ッ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#4S",
      "#11P",
      "よりにもよって、ヴァルドさんが\u0001",
      "クスリに手を出しただァ～ッ！？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#4S",
      "#11P",
      "これもゼンブ……\u0001",
      "ゼンブ、テメエのせいだろォオ！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#6P",
      "#00101F",
      "（ロ、ロイド……\u0001",
      "  彼らにはどこまで教えたの？）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#12P",
      "#00003F",
      "（ああ、さすがに魔人化の\u0001",
      "  ことまでは言っていないけど……）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#6P",
      "#N",
      "#10108F",
      "（……でも、グノーシスを服用した\u0001",
      "  可能性については伝えました。）\u0002"
