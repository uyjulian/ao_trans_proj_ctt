      "\u0007\u0002",
      "#56I",
      "地のセピス?１５０\u0001",
      "\u0007\u0002",
      "#57I",
      "水のセピス?１５０\u0001",
      "\u0007\u0002",
      "#58I",
      "火のセピス?１５０\u0001",
      "\u0007\u0002",
      "#59I",
      "風のセピス?１５０\u0001",
      "\u0007\u0002",
      "#60I",
      "時のセピス?１５０\u0001",
      "\u0007\u0002",
      "#61I",
      "空のセピス?１５０\u0001",
      "\u0007\u0002",
