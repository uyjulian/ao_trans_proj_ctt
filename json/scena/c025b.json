[
  {
    "Text": [
      "あら、いつの間にか\u0001",
      "空がすっかり暗くなったわね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "お父さんも帰ってくるし、\u0001",
      "急いで食事の支度をしないと。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……あっ！\u0001",
      "あたしのファッション誌の棚に\u0001",
      "鉄道の本が混ざってる……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "絶対おとーさんの仕業ね。\u0001",
      "何度おんなじ手を\u0001",
      "使うつもりなのかしら。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "おとーさん、時々こうやって\u0001",
      "あたしを鉄道好きにしようとするの。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "まったく、コシャクなんだから。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "リュウにはいつか問屋の仕事を\u0001",
      "継がせたいと思っておるのじゃが……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "こやつときたら\u0001",
      "遊ぶ事ばかり覚えおってな。\u0001",
      "やれやれじゃわい。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "リュウの奴は日曜学校の成績も\u0001",
      "さっぱりなのじゃよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "遊ばせるばかりじゃなくて、\u0001",
      "少しは勉強させなければのう。\u0002"
    ],
    "Translation": ""
  }
]