      "\u0007\u0002",
      "#56I",
      "地のセピス?３０\u0001",
      "\u0007\u0002",
      "#57I",
      "水のセピス?３０\u0001",
      "\u0007\u0002",
      "#58I",
      "火のセピス?３０\u0001",
      "\u0007\u0002",
      "#59I",
      "風のセピス?３０\u0001",
      "\u0007\u0002",
      "#60I",
      "時のセピス?３０\u0001",
      "\u0007\u0002",
      "#61I",
      "空のセピス?３０\u0001",
      "\u0007\u0002",
