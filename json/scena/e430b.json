  {
    "Text": [
      "#10310F",
      "#4P",
      "#N",
      "──ヴァルド！？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ノエル",
    "Translation": ""
  },
  {
    "Text": [
      "#10107F",
      "#4P",
      "#N",
      "あ、あの娘も……！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ロイド",
    "Translation": ""
  },
  {
    "Text": [
      "#00010F",
      "#4P",
      "#N",
      "クッ、飛行艇",
      "#6Rそんなもの#",
      "まで……！？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00310F",
      "#5S",
      "#24A",
      "#2765V",
      "おおおおおおおおおおっ！！\u0002"
    ],
