      "\u0007\u0002",
      "#56I",
      "地のセピス?８０\u0001",
      "\u0007\u0002",
      "#57I",
      "水のセピス?８０\u0001",
      "\u0007\u0002",
      "#58I",
      "火のセピス?８０\u0001",
      "\u0007\u0002",
      "#59I",
      "風のセピス?８０\u0001",
      "\u0007\u0002",
      "#60I",
      "時のセピス?８０\u0001",
      "\u0007\u0002",
      "#61I",
      "空のセピス?８０\u0001",
      "\u0007\u0002",
      "#62I",
      "幻のセピス?８０\u0001",
      "\u0007\u0000",
