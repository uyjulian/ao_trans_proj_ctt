[
  {
    "Text": "light01",
    "Translation": ""
  },
  {
    "Text": "model05_light",
    "Translation": ""
  },
  {
    "Text": "light01",
    "Translation": ""
  },
  {
    "Text": "model05_light",
    "Translation": ""
  },
  {
    "Text": [
      "やはり、こういうときこそ\u0001",
      "家族で一緒にいなくてはね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ともあれ、息子夫婦が\u0001",
      "無事でよかったわ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "一家の大黒柱である\u0001",
      "息子夫婦が無事でよかったわ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "やはり、こういうときこそ\u0001",
      "家族で一緒にいなくてはね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "大切なのは、どんな時でも\u0001",
      "堂々と構えていることです。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "堂々とさえしていれば、\u0001",
      "他者に伝染しがちな怯えや不安も\u0001",
      "きっと吹き飛んでいくでしょう。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "子供たちも、息子夫婦の事は\u0001",
      "不安でしょうが……\u0001",
      "私がしっかりしていなければね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "堂々とさえしていれば、\u0001",
      "他者に伝染しがちな怯えや不安も\u0001",
      "きっと吹き飛んでいくでしょう。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "子供たちも、息子夫婦の事は\u0001",
      "不安でしょうが……\u0001",
      "私がしっかりしていなければね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "この先、クロスベルは\u0001",
      "大変な事になっていくでしょう。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "でも、私たち家族が一丸になれば\u0001",
      "きっと大丈夫……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "どんなことがあっても\u0001",
      "皆で力をあわせて、\u0001",
      "乗り切っていかなければね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "この先、クロスベルは\u0001",
      "大変な事になっていくでしょう……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "どんなことがあっても\u0001",
      "皆で力をあわせて、\u0001",
      "乗り切っていかなければね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "息子もマリエッタさんも、\u0001",
      "破壊されたＩＢＣに勤務していたから\u0001",
      "かなり忙しくしてるのよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふう、体を壊さないよう\u0001",
      "気をつけて欲しいものだけど。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "武装集団だなんて\u0001",
      "どこから沸いて出たか\u0001",
      "分からないけれど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "罪のない人々を巻き込むなんて\u0001",
      "とても許せないわ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "警備隊には、一刻も早く\u0001",
      "マインツの人々を\u0001",
      "助け出してほしいわね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "罪のない人々を巻き込むなんて\u0001",
      "とても許せないわ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "警備隊には、一刻も早く\u0001",
      "武装集団からマインツの人々を\u0001",
      "助け出してほしいわね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "今日はアンリも\u0001",
      "出かけているみたいね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "雨の日くらい家にいろ、\u0001",
      "とまでは言わないけれど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "濡れて風邪をひかないように\u0001",
      "注意していてほしいわね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "雨の日くらい家にいろ、\u0001",
      "とまでは言わないけれど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "濡れて風邪をひかないように\u0001",
      "注意していてほしいわね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "さっきまでシンディに\u0001",
      "ちょっとした料理のコツを\u0001",
      "教えていたのよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふぅ……\u0001",
      "一仕事終えた後のお茶は格別ね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "さ、あとは焼きあがるのを待つだけ。\u0001",
      "自分だけでやってごらんなさい。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "クロスベルが国家独立する……\u0001",
      "実現すればこの上ない事だと\u0001",
      "あたしも思うけれど。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "当然、帝国と共和国は\u0001",
      "圧力をかけてくるでしょうし\u0001",
      "かなり難しいでしょう。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "それともディーター市長には、\u0001",
      "何かいい策でもあるのかしら……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "クロスベルが国家独立する……\u0001",
      "実現すればこの上ない事だと\u0001",
      "あたしも思うけれど。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ディーター市長には、\u0001",
      "何かいい策でもあるのかしら……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "シンディは今日はおとなりに\u0001",
      "遊びに行っているみたい。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヘイワースさんの奥さんには\u0001",
      "よく、お料理教室などで\u0001",
      "お世話になっているようだし……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "今度お礼をしなくてはいけませんね。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ヘイワースさんの奥さんには\u0001",
      "シンディがよくお世話になっています。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "今度お礼をしなくてはいけませんね。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "マリエッタさんも\u0001",
      "忙しいみたいだし……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "家族全員で食事をするのは\u0001",
      "しばらく先のことになりそうね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "除幕式の様子は\u0001",
      "この住宅街からも見えたのですが……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あれほど高い建造物が建つなど、\u0001",
      "昔では考えられませんでした。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "内部の設備も、旧市庁舎とは\u0001",
      "比べ物にならないほど\u0001",
      "整っているといいますし……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "まさに、あの除幕式は\u0001",
      "歴史の移り変わりを\u0001",
      "表しているようでしたね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "オルキスタワー……\u0001",
      "あれほど高い建造物が建つなど、\u0001",
      "昔では考えられませんでした。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "技術の進歩というものは、\u0001",
      "本当にすごいものですね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "この間引っ越してきた、\u0001",
      "共和国出身の子たち……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "どうやら人に迷惑をかけるのを\u0001",
      "楽しんでいるフシがありますね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふう、嘆かわしい……\u0001",
      "どういう教育をされたのかしら。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "子供がどう成長するかは\u0001",
      "親の育て方一つにかかっています。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "子供が間違った道に行かないよう\u0001",
      "導くのも親の務めだと思うのです。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "フェリックは言葉が荒いし\u0001",
      "礼儀正しいとは言えないけれど、\u0001",
      "思いやりがある優しい子です。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふふ、いい子に育ってくれて\u0001",
      "うれしいわ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "アンリは最近、遊ぶだけでなく\u0001",
      "勉強に対してもかなりのやる気を\u0001",
      "見せるようになりました。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふふ、日曜学校でいい競争相手でも\u0001",
      "できたのかしらね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "よく遊び、よく学ぶ……\u0001",
      "それこそが子供のあるべき姿だと\u0001",
      "あたしは思います。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "アンリには、\u0001",
      "遊びにも勉強にも一所懸命に\u0001",
      "取り組んで欲しいわね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "マリエッタさん、よく戻ってきたわね。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ええ、ＩＢＣ本社が\u0001",
      "あんな事になっちゃいましたから……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "今日はこのままオルキスタワーに\u0001",
      "手伝いに向かう予定です。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そう……忙しいようだけど\u0001",
      "体を壊さないようにね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "へえ、すご～い！\u0001",
      "クッキーってフライパンでも\u0001",
      "焼けちゃうのね！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "生地が張り付かないように\u0001",
      "気をつけてさえいれば、\u0001",
      "基本は同じですよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "道具なんか\u0001",
      "いくらでも代用がきくの。\u0001",
      "覚えておくといいわね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "さっすがお婆ちゃん！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "そういえばユンクス、\u0001",
      "マリエッタさんは帰国の目処は\u0001",
      "ついているのかしら？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "うーん、しばらく難しいかもな。\u0001",
      "結構忙しくしてるみたいだし……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そう……\u0001",
      "家族全員で食事をするのは\u0001",
      "しばらく先のことになりそうね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……あいたた。\u0001",
      "雨の日は節々が痛むわね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ウルスラ病院に\u0001",
      "薬をもらいにいかなくては\u0001",
      "ならないのだけど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "婆ちゃん、大丈夫かい？\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "なんなら俺が病院まで\u0001",
      "付き添おうか。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ふふ、ありがとうフェリック。\u0002",
    "Translation": ""
  },
  {
    "Text": "でも結構よ、１人で行けますから。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "外じゃあんな化物が\u0001",
      "うろついてるけど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "親父たちなら\u0001",
      "きっと大丈夫だよな……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "なんだかクロスベルが\u0001",
      "妙なことになっちまってる\u0001",
      "みたいだなあ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ま、ウチには婆ちゃんがいるし……\u0001",
      "何があっても平気だよな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "母ちゃん、この間の\u0001",
      "記念祭以来だなあ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "へへ、元気そうで\u0001",
      "なによりだぜ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "とんでもない事件が\u0001",
      "起こっちまったなあ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "こりゃ、パーティなんかに\u0001",
      "参加してる場合じゃないぜ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "湿気はスーツの天敵なんだよな。\u0001",
      "雨の日は特に気を遣うよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "パーティに着ていくスーツなんだ、\u0001",
      "大事にしないとな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "シンディが作ったクッキーを\u0001",
      "つまみ食いしたら、行儀が悪いって\u0001",
      "婆ちゃんに叱られちまったよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "うーん、曲がりなりにも社交界に\u0001",
      "出入りしてるのに、行儀が悪いのは\u0001",
      "ちょっと致命的だよな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "今度からは気をつけようっと。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "シンディが作ったクッキーを\u0001",
      "つまみ食いしたら、行儀が悪いって\u0001",
      "婆ちゃんに叱られちまったよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "今度からは気をつけようっと。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "おお、なんだか下から\u0001",
      "甘くていい匂いがしてきたな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "お菓子でも作ってんのかな。\u0001",
      "ひとつまみもらいにいこうっと。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "昨夜行った財界人のパーティでも、\u0001",
      "独立の話題が飛び交ってたよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "けど俺、よく分かってなくて\u0001",
      "いまいち話についていけなくてさ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "婆ちゃんに聞いて、\u0001",
      "どういうことか勉強しとこうかなあ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "今時の話題についていけないんじゃ、\u0001",
      "社交界では取り残されちまう。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "独立のことは\u0001",
      "婆ちゃんに教えてもらおうっと。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ちぇっ、シンディのやつに\u0001",
      "家事を押し付けられちまった。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "まあ、仕方ねえか。\u0001",
      "俺もたまにはちゃんと\u0001",
      "家事をやんないとな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "仕事で忙しい親父が、\u0001",
      "久しぶりに夕飯の時間に\u0001",
      "帰ってきたんだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ありゃ、そういえば\u0001",
      "イスが足りないな。\u0001",
      "物置から出してこないと……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ありゃ、そういえば\u0001",
      "イスが足りないな。\u0001",
      "物置から出してこないと……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "今夜は各国の首脳たちが\u0001",
      "アルカンシェルを観劇するらしい。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そのあとミシュラムの迎賓館で\u0001",
      "ちょっとした会食があるそうだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "いいよなあ。\u0001",
      "俺もいつかそんなパーティに\u0001",
      "参加してみたいぜ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "各国の首脳たちは、\u0001",
      "アルカンシェルを見た後\u0001",
      "迎賓館で会食するらしい。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "いいよなあ。\u0001",
      "俺もいつかそんなパーティに\u0001",
      "参加してみたいぜ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "フフ、シンディ。\u0001",
      "なんだったらお前も\u0001",
      "パーティに参加してみるといい。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "きっと新しい価値観に\u0001",
      "目覚めること請け合いだぜ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ちぇ、婆ちゃんってば\u0001",
      "せっかくおぶってやるって\u0001",
      "言ってるのにさー。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ま、婆ちゃんらしくて\u0001",
      "いいんだけどな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "俺んちの親父、\u0001",
      "ＩＢＣの事業部の主任でさ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "社交界にもコネがあって、\u0001",
      "よくパーティの招待状とかが届くんだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ただ、親父はなかなか忙しくてね。\u0001",
      "せっかくだから俺が代理として\u0001",
      "出席させてもらってるのさ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "いや～、おかげさまで\u0001",
      "社交界のことを色々勉強できるし\u0001",
      "毎日が充実してるぜ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "パーティに参加するのって、\u0001",
      "社交界のことが色々わかって\u0001",
      "楽しいんだよな～。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "これも親父のコネのおかげ。\u0001",
      "へへ、役得ってやつかな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ソフィアさんのご家族も\u0001",
      "アルモリカ村から\u0001",
      "戻ってきてたみたい。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふふ、よかった。\u0001",
      "またお料理を教えて\u0001",
      "もらわなくちゃ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "父さんと母さんも、\u0001",
      "オルキスタワーのＩＢＣ事業部に\u0001",
      "勤めてたんだけど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "仕事に行ったままこんな事が起きて、\u0001",
      "まだ帰ってきていないの。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "どうしよう、大丈夫かな……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "父さんと母さん、\u0001",
      "オルキスタワーに仕事に行ったまま\u0001",
      "帰ってきていないの。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "どうしよう、大丈夫かな……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "ＩＢＣ事業部に勤めてる\u0001",
      "お父さんとお母さんも、\u0001",
      "今は相当忙しいみたいなの。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "資産凍結とかの話が出たから\u0001",
      "色んな方面に影響が\u0001",
      "出ちゃってるんですって。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "こんな時だから\u0001",
      "みんな一緒にいたかったけど……\u0001",
      "仕方ないでしょうね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ＩＢＣ事業部に勤めてる\u0001",
      "お父さんとお母さんも、\u0001",
      "今は相当忙しいみたいなの。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "こんな時だから\u0001",
      "みんな一緒にいたかったけど……\u0001",
      "仕方ないでしょうね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "お母さんが外国から戻ってきたの。\u0001",
      "……なんだか安心しちゃった。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あんな事件があったんだし、\u0001",
      "家族が離れ離れだと不安だもんね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "事件のニュースを聞いて\u0001",
      "びっくりしちゃったわよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "警備隊の装甲車も\u0001",
      "何台も通ってるみたいだし……\u0001",
      "うう、やっぱり怖いわ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "今日はお料理教室もないし……\u0001",
      "おうちでゆっくりとしてようかな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そうだ、昨日作ったみたいな\u0001",
      "クッキーのアレンジを研究するのも\u0001",
      "楽しいかもしれないわね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "クッキーの生地に\u0001",
      "チョコレートチップを\u0001",
      "混ぜてみようかな～。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふふ、完成したら\u0001",
      "お婆ちゃんも呼んで\u0001",
      "優雅なティータイムね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ソフィアさんちに\u0001",
      "クッキーを届けに行く途中で、\u0001",
      "救急車のサイレンが聞こえたの。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "急いで見にいったら、\u0001",
      "結構な台数が通っていって……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "やだなあ、大きな事故でも\u0001",
      "あったのかしら。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……まあいっか。\u0001",
      "深く考えないようにしよっと。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "とにかく、コリン君が\u0001",
      "私の作ったクッキーを\u0001",
      "喜んでくれて良かったわ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "さっすがお婆ちゃんね。\u0001",
      "オーブンを使わなくても\u0001",
      "クッキーができちゃうなんて。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふふ、あとでソフィアさんに\u0001",
      "出来栄えを見てもらおうっと。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "あれー、オーブンが壊れてる……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "せっかくソフィアさんに\u0001",
      "クッキーの焼き方を教わったのに、\u0001",
      "これじゃあ焼けないよ～……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "はあ、せっかくソフィアさんに\u0001",
      "クッキーの焼き方を教わったのに。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "オーブンが壊れてるんじゃ、\u0001",
      "当分は実践できないよ～……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "アンリ、オルキスタワーを\u0001",
      "近くで見てきたんでしょ。\u0001",
      "どんな感じだった？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "やっぱり大っきかった？\u0001",
      "ミラがかかってそうだった？\u0001",
      "いっぱい人いた？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ちょ、ちょっと姉さん。\u0001",
      "一度にそんなに質問されても\u0001",
      "答えられないってば～。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "いいじゃない、\u0001",
      "ケチケチしないで教えてよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "アンリは友達と一緒に\u0001",
      "オルキスタワーを\u0001",
      "見に行ったみたい。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "やー、わざわざ\u0001",
      "混んでるときに行くなんて、\u0001",
      "子供ってパワフルよね～。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "お兄ちゃん、\u0001",
      "今夜もパーティなの？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "もう、よく飽きないわねえ。\u0002",
    "Translation": ""
  },
  {
    "Text": "うん、いい味付け☆\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "私ったら、どんどん料理が\u0001",
      "上手になっていくわね～。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふふ、これもソフィアさんの\u0001",
      "教えのおかげかな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "私、お隣のソフィアさんが\u0001",
      "定期的に開いてる\u0001",
      "お料理教室に通ってるの。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふふ、おかげさまで\u0001",
      "料理の腕がかなり上達したわ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "私たちは、職員たちと一緒に\u0001",
      "オルキスタワーに\u0001",
      "閉じ込められていたんだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "子供達やお袋には\u0001",
      "心配をかけてしまったが、\u0001",
      "何とか帰ってこれてよかったよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "この間の襲撃事件みたいなことが\u0001",
      "またいつ起きるとも分からない。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "いや、むしろ何が起きても\u0001",
      "不思議じゃない状況なのかもしれない。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "私も一家の大黒柱として、\u0001",
      "しっかりと家族を守っていかないとな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "妻のマリエッタは\u0001",
      "外国のＩＢＣ支社で働いてるんだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あっちはあっちで忙しそうだし、\u0001",
      "帰ってくるのはまだまだ先だろうな……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "私たちがいない間、\u0001",
      "お義母さんが子供達を\u0001",
      "勇気付けてくれていたみたい。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふふ、お義母さんには\u0001",
      "本当に頭が上がらないわね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あんな演説があったからね、\u0001",
      "夫と２人で特別に許可をとって、\u0001",
      "家の様子を見に来たの。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふふ、頼りになるお義母さんで\u0001",
      "本当に助かっちゃうわ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ふふ、息子たちも義母さんも、\u0001",
      "襲撃で怪我なんてしてなくて\u0001",
      "本当に良かったわ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "さてと……はやく\u0001",
      "ＩＢＣ事業部の手伝いに\u0001",
      "行かないとね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "政府支給の食品も、\u0001",
      "戒厳令から入ってきてないんです。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "この状況が続いたら厳しいですよね……\u0002",
    "Translation": ""
  },
  {
    "Text": "もぐもぐ……\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "あ、支援課の皆さん。\u0001",
      "ウチになにか用ですか？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "よかったら一緒に\u0001",
      "夕飯を食べていきます？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "それにしても昼の除幕式は\u0001",
      "すごかったですよね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "警察のみなさんは\u0001",
      "忙しいでしょうけど、\u0001",
      "がんばってくださいね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "昨日は日曜学校で\u0001",
      "たくさん宿題が出たんです。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "最近はテストの点でも\u0001",
      "キーアちゃんに負けてますし、\u0001",
      "きちんと自習もしないと。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "『大切なのは自習だ』って、\u0001",
      "おばあちゃんも言ってました。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "次はテストで\u0001",
      "キーアちゃんに勝てるように、\u0001",
      "しっかり勉強しないと。\u0002"
    ],
    "Translation": ""
  }
]