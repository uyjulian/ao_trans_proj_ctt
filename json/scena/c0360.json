[
  {
    "Text": "hikari00_add",
    "Translation": ""
  },
  {
    "Text": "hikari01_add",
    "Translation": ""
  },
  {
    "Text": "hikari00_add",
    "Translation": ""
  },
  {
    "Text": "hikari01_add",
    "Translation": ""
  },
  {
    "Text": "",
    "Translation": ""
  },
  {
    "Text": [
      "\u0007\u0005",
      "車雑誌『月間カーマニアvol.3』がある。",
      "\u0007\u0000",
      "\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "",
    "Translation": ""
  },
  {
    "Text": [
      "ペイントパターン\u0001",
      "\u0007\u0002",
      "『ディープカラー』",
      "\u0007\u0000",
      "を覚えた。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヒヒャヒャ、それにしても……\u0001",
      "あんたたちもご苦労だったねえ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "おかげさまで、この姉妹人形に\u0001",
      "ようやく買い手がつくかもしれないよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00101F",
      "その……イメルダさんは\u0001",
      "怖くないんですか？\u0002",
      "\u0003",
      "#00106F",
      "化けて出てくるような人形を\u0001",
      "売り払ったりしたら、\u0001",
      "た、祟られたりするんじゃ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヒヒャヒャ、オバケが怖くて\u0001",
      "こんな商売をやってられるかい！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "この倉庫に保管しているものにも、\u0001",
      "いくつか妖しい品があるからねえ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "何なら、持ち主が次々と不幸になるっていう\u0001",
      "いわくつきの指輪でも触ってみるかい？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00012F",
      "け、結構です。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00203F",
      "（……そういう品が一箇所に\u0001",
      "  集まっていたせいで、霊的な場が\u0001",
      "  生まれたのかもしれませんね。）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00306F",
      "（け、結局このバーさんが原因かよ……）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "おかげさまで、この姉妹人形に\u0001",
      "ようやく買い手がつくかもしれないよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヒヒャヒャ、あんたたちには\u0001",
      "感謝させてもらわないとねえ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00000F",
      "すみません、どなたか\u0001",
      "いらっしゃいますか？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00305F",
      "……やっぱだれも\u0001",
      "いないんじゃねえか？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00105F",
      "でも、届け先の住所は\u0001",
      "確かにここのはずよ。\u0002",
      "\u0003",
      "#00103F",
      "玄関にも鍵は\u0001",
      "かかってなかったし……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "女の子の声",
    "Translation": ""
  },
  {
    "Text": "……おにいちゃんたち、だあれ？\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "#10105F",
      "あ……？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00105F",
      "ひ、人が……？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "え、えっと……\u0001",
      "お嬢ちゃん、今お家に一人かい？\u0002",
      "\u0003",
      "#00000F",
      "お兄さんたちは\u0001",
      "荷物を届けに来たんだけど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あっ、もしかして……\u0001",
      "届いたのかしら！？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "ね、上がって上がって！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "#10309F",
      "ハハ、ろくに話を\u0001",
      "聞いていない感じだね。\u0002",
      "\u0003",
      "#10300F",
      "察するに、あの子宛ての\u0001",
      "荷物なのかな？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00106F",
      "お、おかしいわ……\u0002",
      "\u0003",
      "#00101F",
      "あんな子がいたなんて\u0001",
      "聞いたことないもの。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00306F",
      "しかし……現実にこうして\u0001",
      "出会っちまったわけだしなあ。\u0002",
      "\u0003",
      "#00301F",
      "一体どういうことなんだ？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00003F",
      "分からないけど……\u0002",
      "\u0003",
      "#00000F",
      "とりあえず、\u0001",
      "中に入ってみよう。\u0002",
      "\u0003",
      "あの子からなにか\u0001",
      "聞けるかもしれないしね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00000F",
      "えっと、これが届けにきた荷物だよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "",
    "Translation": ""
  },
  {
    "Text": [
      "\u001F\u033A",
      "\u0007\u0000",
      "を渡した。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "わ～い、やっと帰ってきたぁ！\u0002",
    "Translation": ""
  },
  {
    "Text": "……うんしょ、と。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "#00305F",
      "（……随分ボロそうな人形だな。）\u0002",
      "\u0003",
      "（こいつもローゼンベルク製の\u0001",
      "  アンティークドールってやつか？）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00103F",
      "（ううん、そこまで高価なものじゃ\u0001",
      "  ないみたいだけど……）\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00002F",
      "もしかして、この人形は\u0001",
      "君のものなのかい？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "モノじゃないよ～！\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "だいじなだいじな妹なの。\u0001",
      "うふふ、いいこいいこ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00106F",
      "そうなんだ……\u0002",
      "\u0003",
      "#00109F",
      "え、ええっと。\u0001",
      "ちゃんと届いてよかったわね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "うん、ありがとうね。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "それじゃ、ちょっとこの子を\u0001",
      "わたしの部屋に連れてってくるね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10103F",
      "うーん……\u0001",
      "結局なんなんでしょう。\u0002",
      "\u0003",
      "#10100F",
      "家に他の家族がいる\u0001",
      "気配もありませんし……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00203F",
      "………………………………\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "……？\u0001",
      "ティオ、どうした？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00203F",
      "……いえ、多分\u0001",
      "気のせいだとは思うんですが……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "老婆の声",
    "Translation": ""
  },
  {
    "Text": [
      "#4S",
      "──これっ、だれかいるのかい！？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "……い、今の声は……？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00105F",
      "お客さん……かしら？\u0002",
      "\u0003",
      "親御さんは\u0001",
      "留守にしてるみたいだし、\u0001",
      "私たちが出てあげましょうか。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00000F",
      "あ、ああ……そうだな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "あ……\u0001",
      "アンティークショップの。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "おや、アンタたち……\u0001",
      "こんな所で何をしてるんだい？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00012F",
      "えっとまあ、\u0001",
      "仕事で伺ったところ\u0001",
      "だったんですけど……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00105F",
      "このお宅に何か御用でしょうか？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……仕事？\u0001",
      "何を言ってるんだい。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "この家には\u0001",
      "１０年以上前から\u0001",
      "だれも入居してないよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "しばらくアタシの\u0001",
      "アンティークショップの\u0001",
      "物置になってるくらいさ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……なんだい、\u0001",
      "その腑に落ちないような顔は。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00305F",
      "……ど、どういうことだ？\u0002",
      "\u0003",
      "婆さんの所有する\u0001",
      "物件だったのはいいとしても……\u0002",
      "\u0003",
      "#00303F",
      "俺たちゃ、確かにさっき\u0001",
      "女の子と話してたよな？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10306F",
      "そのはずだけどね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10101F",
      "……ね、念のため\u0001",
      "さっきの女の子に\u0001",
      "話を聞いてみましょう。\u0002",
      "\u0003",
      "何か事情があるのかも\u0001",
      "しれませんし。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00003F",
      "そ、そうだな……\u0002",
      "\u0003",
      "#00001F",
      "たしかリビングの奥の部屋に\u0001",
      "行ってたはずだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "これこれ、アタシを無視して\u0001",
      "話を進めるんじゃないよ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00205F",
      "………………………………\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "あ、あれ？\u0001",
      "さっきの女の子はどこに……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00305F",
      "どっかに隠れてんのか……？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00105F",
      "ロ、ロイド……\u0001",
      "あ、あ、あれ見て……！\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "え……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "あの人形……\u0001",
      "片方は俺たちが届けた\u0001",
      "モノだけど……\u0002",
      "\u0003",
      "#00011F",
      "も、もう片方って……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10301F",
      "さっきの女の子に瓜二つだね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "……アンタたち。\u0001",
      "いい加減アタシにも\u0001",
      "事情を説明したらどうだい。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "事と次第によっちゃ、\u0001",
      "不法侵入で訴えてもいいんだよ？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00006F",
      "わ、わかりました。\u0001",
      "実は、その……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "",
    "Translation": ""
  },
  {
    "Text": [
      "\u0007\u0005",
      "荷物を届けに訪れたことと、\u0001",
      "出迎えた少女が人形の片方に\u0001",
      "瓜二つなことを説明した。",
      "\u0007\u0000",
      "\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "………………………………\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "……ヒヒャヒャ、なるほどねえ。\u0001",
      "ヤツの言っていたような事が\u0001",
      "まさに起こったってわけだ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10105F",
      "え、えっと……どういうことですか？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "そこの人形の片方はね、\u0001",
      "昔、アタシが外国で\u0001",
      "仕入れてきたモンさ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "どこぞの旧い名家の娘たちに\u0001",
      "似せて作られたという、\u0001",
      "いわくつきの物でね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "娘たちは双子だったから\u0001",
      "２体１組の人形が作られたそうだが、\u0001",
      "入手できたのは片方だけでねえ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "悪い品じゃないんだが、\u0001",
      "不思議と買い手がつかない\u0001",
      "妙な人形だったのさ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "……えっと、その話に\u0001",
      "一体何の関係が……？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "やれやれ、意外と鈍いねえ。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "要するにだ、\u0001",
      "人形がこの場所に届いたことや\u0001",
      "妙な少女が現れたこと……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "それらは人形自身が\u0001",
      "命を持って成したことかも\u0001",
      "しれないってことさね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00011F",
      "#4S",
      "……はあ！？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "昔から『人形には命が宿る』なんて\u0001",
      "うそぶいてるヤツがいてねえ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヒヒャヒャ、案外本当の\u0001",
      "ことだったのかもしれないねえ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "",
    "Translation": ""
  },
  {
    "Text": [
      "\u0007\u0005",
      "\u0006\u0018前編の人形工房クエストを？（テスト用）",
      "\u0007\u0000",
      "\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "【変更しない】\u0001",
      "【クリアしている】\u0001",
      "【クリアしていない】\u0001"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "命が宿るって……\u0002",
      "\u0003",
      "#00003F",
      "た、確かに以前ヨルグ老人\u0001",
      "そんなことを言ってたような\u0001",
      "気がしますけど。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "命が宿るって……\u0002",
      "\u0003",
      "#00006F",
      "さすがにありえない\u0001",
      "話のような気がしますけど。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00105F",
      "そ、それじゃあ……\u0001",
      "人形のお化けが出たって\u0001",
      "ことですか！？\u0002",
      "\u0003",
      "#00106F",
      "そ、そんな事って……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00203F",
      "まあ、真偽はともかく……\u0002",
      "\u0003",
      "#00200F",
      "どうやらこの屋敷は\u0001",
      "何らかの霊的な“場”と\u0001",
      "なっているようです。\u0002",
      "\u0003",
      "例の《塔》や《僧院》と同じ、\u0001",
      "上位三属性の気配というか……\u0002",
      "\u0003",
      "それが何らかの霊的な現象を\u0001",
      "引き起こしたのかもしれません。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00105F",
      "れ、霊的って……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10302F",
      "それでなんだか\u0001",
      "様子がおかしかったわけか。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00301F",
      "にわかには信じられねえ\u0001",
      "話だと思ったが……\u0002",
      "\u0003",
      "#00306F",
      "ティオすけが言うと\u0001",
      "マジに思えてくるよな。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#10112F",
      "ま、まさかって\u0001",
      "感じですけどね……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヒヒャヒャ……！\u0001",
      "なかなかリアクションが\u0001",
      "いいじゃないか。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ま、所詮は世捨て人の戯言だし\u0001",
      "信じようも信じまいも\u0001",
      "アンタたちの勝手だが……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": "一つだけ、確かなことがあるよ。\u0002",
    "Translation": ""
  },
  {
    "Text": [
      "#00005F",
      "そ、それは……？\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "あの姉妹人形が揃った今、\u0001",
      "やっとこさ買い手がつくかも\u0001",
      "しれないってことさね。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "ヒヒャヒャ……\u0001",
      "こりゃあとんだ拾いモン\u0001",
      "だったねえ。\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00306F",
      "身も蓋もない婆さんだなあ……\u0002"
    ],
    "Translation": ""
  },
  {
    "Text": [
      "#00006F",
      "ま、まあともかく……\u0001",
      "これで荷物は届け終わったんだ。\u0002",
      "\u0003",
      "#00000F",
      "カプア特急便の人に、\u0001",
      "報告に行くとしよう。\u0002"
    ],
